<h1>Show Estado</h1>

<ul>

  <li>
    <strong>Nome:</strong>
    <%= @estado.nome %>
  </li>

  <li>
    <strong>Uf:</strong>
    <%= @estado.uf %>
  </li>

</ul>

<span><%= link "Editar", to: Routes.estado_path(@conn, :edit, @estado) %></span>
<span><%= link "Voltar", to: Routes.estado_path(@conn, :index) %></span>
